{
  "name": "keep-react",
  "version": "1.2.0",
  "description": "Official React components built for React and Tailwind CSS",
  "license": "MIT",
  "keywords": [
    "design-system",
    "keep-design-system",
    "keep-react",
    "react",
    "tailwind",
    "tailwindcss",
    "tailwind-css"
  ],
  "homepage": "https://react.keepdesign.io/",
  "bugs": "https://github.com/StaticMania/keep-react/issues",
  "files": [
    "./lib/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "default": "./lib/cjs/index.js"
      }
    },
    "./preset": {
      "default": "./lib/cjs/theme/keep-preset.js"
    },
    "./css": {
      "default": "./lib/cjs/theme/main.min.css"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/StaticMania/keep-react"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "newApp": "rm -rf node_modules && rm -rf .next && yarn install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:lib": "rm -rf ./lib/ && yarn build:lib:dev",
    "build:lib:dev": "yarn build:lib:esm && yarn build:lib:cjs && yarn build:css",
    "build:css": "css-minify -f ./app/main.css -o ./lib/cjs/theme",
    "build:lib:cjs": "tsc -p tsconfig.lib.json",
    "build:lib:esm": "tsc -p tsconfig.lib.json --module esnext --declaration --outDir ./lib/esm",
    "lint": "eslint app --ext ts,tsx,js,jsx,cjs --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint app --ext js,jsx,ts,tsx,cjs --fix",
    "format": "prettier --write 'app/**/*.{js,jsx,ts,tsx,css,html}'"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.4",
    "@headlessui/react": "^1.7.17",
    "date-fns": "^3.6.0",
    "embla-carousel": "^8.0.2",
    "embla-carousel-react": "^8.0.2",
    "rc-slider": "^10.5.0",
    "rc-tooltip": "^6.1.3",
    "react": "^18.3.1",
    "react-collapse": "^5.1.1",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-focus-lock": "^2.11.2",
    "react-remove-scroll": "^2.5.9",
    "recharts": "^2.10.3",
    "sonner": "^1.4.41",
    "next-themes": "^0.3.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "embla-carousel-autoplay": "^8.0.2",
    "embla-carousel-class-names": "^8.0.2",
    "phosphor-react": "^1.4.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "@babel/core": "^7.23.6",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.0.4",
    "@tailwindcss/forms": "^0.5.7",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/mdx": "^2.0.10",
    "@types/node": "^20.10.5",
    "@types/rc-slider": "^9.3.1",
    "@types/react": "^18.2.45",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "^18.2.18",
    "@types/react-slider": "^1.3.6",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/recharts": "^1.8.29",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.16",
    "css-minify": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.1",
    "eslint-plugin-storybook": "^0.6.15",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "next": "^14.2.3",
    "next-nprogress-bar": "^2.1.2",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.1",
    "prettier-plugin-css-order": "^2.0.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-slug": "^6.0.0",
    "remark-toc": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "uuid": "^9.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run format"
    }
  },
  "lint-staged": {
    "app/**/*.{ts,tsx,js,jsx}": [
      "npm run lint",
      "npm run format"
    ]
  }
}
